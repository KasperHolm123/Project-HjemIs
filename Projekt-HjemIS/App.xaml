<Application x:Class="Projekt_HjemIS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Projekt_HjemIS"
             xmlns:viewmodels="clr-namespace:Projekt_HjemIS.ViewModels"
             xmlns:views="clr-namespace:Projekt_HjemIS.Views"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodels:MainViewModel}">
            <local:dashboard />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:EditCustomerViewModel}">
            <views:EditCustomerView />
        </DataTemplate>
    </Application.Resources>
</Application>
